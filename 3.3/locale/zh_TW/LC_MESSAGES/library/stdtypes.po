msgid "``s * n`` or ``n * s``"
msgstr ""

#: ../../../library/stdtypes.rst:858
msgid "*n* shallow copies of *s* concatenated"
msgstr ""

#: ../../../library/stdtypes.rst:858
msgid "(2)(7)"
msgstr ""

#: ../../../library/stdtypes.rst:861
msgid "``s[i]``"
msgstr ""

#: ../../../library/stdtypes.rst:861
msgid "*i*\\ th item of *s*, origin 0"
msgstr ""

#: ../../../library/stdtypes.rst:863
msgid "``s[i:j]``"
msgstr ""

#: ../../../library/stdtypes.rst:863
msgid "slice of *s* from *i* to *j*"
