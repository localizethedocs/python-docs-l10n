msgstr ""

#: ../../../library/codecs.rst:854
msgid "``U-00000800`` ... ``U-0000FFFF``"
msgstr ""

#: ../../../library/codecs.rst:854
msgid "1110xxxx 10xxxxxx 10xxxxxx"
msgstr ""

#: ../../../library/codecs.rst:856
msgid "``U-00010000`` ... ``U-0010FFFF``"
msgstr ""

