"    return x+y+0j\n"
"\n"
"server = SimpleXMLRPCServer((\"localhost\", 8000))\n"
"print \"Listening on port 8000...\"\n"
"server.register_function(add, 'add')\n"
"\n"
"server.serve_forever()"
