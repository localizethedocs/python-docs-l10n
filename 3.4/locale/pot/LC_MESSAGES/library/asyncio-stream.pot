#: ../../../library/asyncio-stream.rst:262
msgid "TCP echo client using the :func:`asyncio.open_connection` function::"
msgstr ""

#: ../../../library/asyncio-stream.rst:264
msgid ""
"import asyncio\n"
"\n"
"@asyncio.coroutine\n"
"def tcp_echo_client(message, loop):\n"
"    reader, writer = yield from asyncio.open_connection('127.0.0.1', 8888,\n"
"                                                        loop=loop)\n"
"\n"
"    print('Send: %r' % message)\n"
"    writer.write(message.encode())\n"
"\n"
"    data = yield from reader.read(100)\n"
"    print('Received: %r' % data.decode())\n"
"\n"
"    print('Close the socket')\n"
"    writer.close()\n"
"\n"
"message = 'Hello World!'\n"
"loop = asyncio.get_event_loop()\n"
"loop.run_until_complete(tcp_echo_client(message, loop))\n"
"loop.close()"
msgstr ""

#: ../../../library/asyncio-stream.rst:287
msgid ""
"The :ref:`TCP echo client protocol <asyncio-tcp-echo-client-protocol>` "
"example uses the :meth:`BaseEventLoop.create_connection` method."
msgstr ""

#: ../../../library/asyncio-stream.rst:294
msgid "TCP echo server using streams"
msgstr ""

#: ../../../library/asyncio-stream.rst:296
msgid "TCP echo server using the :func:`asyncio.start_server` function::"
msgstr ""

#: ../../../library/asyncio-stream.rst:298
msgid ""
"import asyncio\n"
"\n"
"@asyncio.coroutine\n"
"def handle_echo(reader, writer):\n"
"    data = yield from reader.read(100)\n"
"    message = data.decode()\n"
"    addr = writer.get_extra_info('peername')\n"
"    print(\"Received %r from %r\" % (message, addr))\n"
"\n"
"    print(\"Send: %r\" % message)\n"
"    writer.write(data)\n"
"    yield from writer.drain()\n"
"\n"
"    print(\"Close the client socket\")\n"
"    writer.close()\n"
"\n"
"loop = asyncio.get_event_loop()\n"
"coro = asyncio.start_server(handle_echo, '127.0.0.1', 8888, loop=loop)\n"
"server = loop.run_until_complete(coro)\n"
"\n"
"# Serve requests until Ctrl+C is pressed\n"
"print('Serving on {}'.format(server.sockets[0].getsockname()))\n"
"try:\n"
"    loop.run_forever()\n"
"except KeyboardInterrupt:\n"
"    pass\n"
"\n"
"# Close the server\n"
"server.close()\n"
"loop.run_until_complete(server.wait_closed())\n"
"loop.close()"
msgstr ""

#: ../../../library/asyncio-stream.rst:332
msgid ""
"The :ref:`TCP echo server protocol <asyncio-tcp-echo-server-protocol>` "
"example uses the :meth:`BaseEventLoop.create_server` method."
msgstr ""

#: ../../../library/asyncio-stream.rst:337
msgid "Get HTTP headers"
msgstr ""

#: ../../../library/asyncio-stream.rst:339
msgid ""
"Simple example querying HTTP headers of the URL passed on the command line::"
msgstr ""

#: ../../../library/asyncio-stream.rst:341
msgid ""
"import asyncio\n"
"import urllib.parse\n"
"import sys\n"
"\n"
"@asyncio.coroutine\n"
"def print_http_headers(url):\n"
"    url = urllib.parse.urlsplit(url)\n"
"    if url.scheme == 'https':\n"
"        connect = asyncio.open_connection(url.hostname, 443, ssl=True)\n"
"    else:\n"
"        connect = asyncio.open_connection(url.hostname, 80)\n"
"    reader, writer = yield from connect\n"
"    query = ('HEAD {path} HTTP/1.0\\r\\n'\n"
"             'Host: {hostname}\\r\\n'\n"
"             '\\r\\n').format(path=url.path or '/', hostname=url.hostname)\n"
"    writer.write(query.encode('latin-1'))\n"
"    while True:\n"
"        line = yield from reader.readline()\n"
"        if not line:\n"
"            break\n"
"        line = line.decode('latin1').rstrip()\n"
"        if line:\n"
"            print('HTTP header> %s' % line)\n"
"\n"
"    # Ignore the body, close the socket\n"
"    writer.close()\n"
"\n"
"url = sys.argv[1]\n"
"loop = asyncio.get_event_loop()\n"
"task = asyncio.ensure_future(print_http_headers(url))\n"
"loop.run_until_complete(task)\n"
"loop.close()"
msgstr ""

