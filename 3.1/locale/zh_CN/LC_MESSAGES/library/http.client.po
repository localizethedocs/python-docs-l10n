#: ../../../library/http.client.rst:496
msgid "Examples"
msgstr ""

#: ../../../library/http.client.rst:498
msgid "Here is an example session that uses the ``GET`` method::"
msgstr ""

#: ../../../library/http.client.rst:500
msgid ""
">>> import http.client\n"
">>> conn = http.client.HTTPConnection(\"www.python.org\")\n"
">>> conn.request(\"GET\", \"/index.html\")\n"
">>> r1 = conn.getresponse()\n"
">>> print(r1.status, r1.reason)\n"
"200 OK\n"
">>> data1 = r1.read()\n"
">>> conn.request(\"GET\", \"/parrot.spam\")\n"
">>> r2 = conn.getresponse()\n"
">>> print(r2.status, r2.reason)\n"
"404 Not Found\n"
">>> data2 = r2.read()\n"
">>> conn.close()"
msgstr ""

#: ../../../library/http.client.rst:514
msgid ""
"Here is an example session that uses ``HEAD`` method. Note that ``HEAD`` "
"method never returns any data. ::"
msgstr ""

#: ../../../library/http.client.rst:518
msgid ""
">>> import http.client\n"
">>> conn = http.client.HTTPConnection(\"www.python.org\")\n"
">>> conn.request(\"HEAD\",\"/index.html\")\n"
">>> res = conn.getresponse()\n"
">>> print(res.status, res.reason)\n"
"200 OK\n"
">>> data = res.read()\n"
">>> print(len(data))\n"
"0\n"
">>> data == b''\n"
"True"
msgstr ""

#: ../../../library/http.client.rst:530
msgid "Here is an example session that shows how to ``POST`` requests::"
msgstr ""

#: ../../../library/http.client.rst:532
msgid ""
">>> import http.client, urllib.parse\n"
">>> params = urllib.parse.urlencode({'spam': 1, 'eggs': 2, 'bacon': 0})\n"
">>> headers = {\"Content-type\": \"application/x-www-form-urlencoded\",\n"
"...            \"Accept\": \"text/plain\"}\n"
">>> conn = http.client.HTTPConnection(\"musi-cal.mojam.com:80\")\n"
">>> conn.request(\"POST\", \"/cgi-bin/query\", params, headers)\n"
">>> response = conn.getresponse()\n"
">>> print(response.status, response.reason)\n"
"200 OK\n"
">>> data = response.read()\n"
">>> conn.close()"
msgstr ""

